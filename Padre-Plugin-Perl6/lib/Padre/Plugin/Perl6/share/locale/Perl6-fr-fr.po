#: lib/Padre/Plugin/Perl6/Perl6Document.pm:323
msgid "Use ~ instead of . for string concatenation"
msgstr "Utiliser ~ à la place de . pour la concaténation de chaînes"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:342
msgid "Use . for method call"
msgstr "Utiliser . pour les appels de méthode"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:361
msgid "Use Perl 6 constructor syntax"
msgstr "Utiliser la syntaxe de construction Perl 6"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:381
msgid "Use loop (;;) for looping"
msgstr "Utiliser loop(;;) pour les boucles"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:400
msgid "Use [*-1] to access final element"
msgstr "Utiliser [*-1] pour accéder au dernier élément"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:419
msgid "Use N.pick for a random number"
msgstr "Utiliser N.pick pour un nombre aléatoire"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:436
msgid "Use (1..N).pick for a random number"
msgstr "Utiliser (1..N).pick pour un nombre aléatoire"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:455
msgid "Use [N..*] for indefinite range"
msgstr "Utiliser [N..*] pour une plage indéfinie"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:474
msgid "Use !! for conditional operator"
msgstr "Utiliser !! pour l'opérateur conditionnel"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:493
msgid "Use ?? (...) !! to avoid precedence bugs"
msgstr "Utiliser ?? (...) !! pour éviter les bugs de précédence"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:513
msgid "Use ?? !! for the conditional operator"
msgstr "Utiliser ?? !! pour l'opérateur conditionnel"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:532
msgid "Use ~= for string concatenation"
msgstr "Utiliser ~= pour la concaténation de chaîne"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:551
msgid "Use ~~ for pattern matching"
msgstr "Utiliser ~~ pour la recherche de motif"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:570
msgid "Use !~~ for negated pattern matching"
msgstr "Utiliser !~~ pour la recherche de motif inversée"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:589
msgid "Use +> for numeric right shift"
msgstr "Utiliser +> pour shift droit numérique"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:606
msgid "Use ~> for string right shift"
msgstr "Utiliser ~> pour shift droit de chaîne"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:624
msgid "Use +< for numeric left shift"
msgstr "Utiliser +< pour shift gauche numérique"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:641
msgid "Use ~< for string left shift"
msgstr "Utiliser ~< pour shift gauche de chaîne"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:660
msgid "Use $! for eval errors"
msgstr "Utiliser $! pour les erreurs d'évaluation"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:679
msgid "Use $::PERL_VERSION"
msgstr "Utiliser $::PERL_VERSION"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:708
msgid "Comment error line"
msgstr "Erreur de commentaire ligne"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:742
msgid "Surround with try { ... }"
msgstr "Encadrer avec try { ... }"

#: lib/Padre/Plugin/Perl6/Perl6Document.pm:770
